#include <bits/stdc++.h>
using namespace std;


int main()
{
    int tc;
    string s;
    cin>>tc;
    while(tc--)
    {
        cin>>s;
        int l=s.size();
        if(s[l-1] == '5')
            {
			cout<<"-"<<endl;
			}
		else if(s[0] == '9' && s[l-1] == '4')
            {
			cout<<"*"<<endl;
        }
		else if(s[0] == '1' && l >= 3)
            {
			cout<<"?"<<endl;
        }
		else if(s=="1"||s=="4"||s=="78"){
            cout<<"+"<<endl;
            }
    }
    return 0;
}
